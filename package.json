{
  "name": "socialmediaai-web-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:staging": "vite --mode staging",
    "dev:production": "vite --mode production",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:staging": "run-p type-check \"build-only -- --mode staging\"",
    "build:production": "run-p type-check \"build-only -- --mode production\"",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "knip": "knip",
    "i18n:unused": "node scripts/find-unused-i18n-keys.js",
    "prepare": "husky",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "verify": "./scripts/verify.sh",
    "quick-check": "./scripts/quick-check.sh"
  },
  "dependencies": {
    "@sentry/vue": "^10.27.0",
    "@vuepic/vue-datepicker": "^12.0.5",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "material-symbols": "^0.40.0",
    "pinia": "^3.0.3",
    "posthog-js": "^1.298.1",
    "vue": "^3.5.22",
    "vue-chartjs": "^5.3.3",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@sentry/vite-plugin": "^4.6.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.33.0",
    "eslint-plugin-oxlint": "~1.11.0",
    "eslint-plugin-vue": "~10.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "knip": "^5.71.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.11.0",
    "prettier": "3.6.2",
    "typescript": "~5.9.0",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-tsc": "^3.1.0"
  }
}
